# Path to a securely stored private key file
PRIVATE_KEY_PATH=/path/to/private_key.json  # Replace with the actual file path

# RPC and WebSocket endpoints for interacting with the Solana network
RPC_ENDPOINT=https://rpc.shyft.to?api_key=XXXXXXXXXXX
RPC_WEBSOCKET_ENDPOINT=wss://rpc.shyft.to?api_key=XXXXXXXXXXXXX

# Token configuration
QUOTE_MINT=WSOL  # Token to be used for quotes (e.g., WSOL or USDC)
QUOTE_AMOUNT=0.01  # Amount of quote token to use per transaction

# Commitment level for Solana transactions
COMMITMENT_LEVEL=finalized

# Snipe list settings
USE_SNIPE_LIST=false  # Set to true if you want to use a snipe list
SNIPE_LIST_REFRESH_INTERVAL=20000  # Interval (in ms) to refresh the snipe list

# Mint renounce checking
CHECK_IF_MINT_IS_RENOUNCED=true  # Set to true to check if a mint has been renounced

# Auto-sell settings
AUTO_SELL=true  # Set to true to enable auto-selling
MAX_SELL_RETRIES=5  # Max number of retries for auto-sell
AUTO_SELL_DELAY=30000  # Delay (in ms) between auto-sell attempts

# Log level for bot output
LOG_LEVEL=info  # Can be 'info', 'debug', or 'error'

# Profit and loss thresholds
TAKE_PROFIT=200  # Take profit threshold in percentage
STOP_LOSS=50  # Stop loss threshold in percentage

# API key for external services like BirdEye
BIRDEYE_API_KEY=4a9711aeXXXXXXXXXXXXXXXXXXXX  # Replace with your actual API key

# Minimum pool size for liquidity sniping
MIN_POOL_SIZE=3  # Minimum size of the pool to consider for sniping
