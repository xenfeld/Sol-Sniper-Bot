# REMOVE private key from here and handle securely (e.g., through a separate key management solution)
PRIVATE_KEY_PATH=/path/to/private_key.json  # Path to a securely stored private key file (recommend using a secure vault or HSM)

# RPC and WebSocket endpoints
RPC_ENDPOINT=https://rpc.shyft.to?api_key=XXXXXXXXXXX
RPC_WEBSOCKET_ENDPOINT=wss://rpc.shyft.to?api_key=XXXXXXXXXXXXX

# Token configuration
QUOTE_MINT=WSOL  # or USDC, depending on your preference
QUOTE_AMOUNT=0.01
MIN_POOL_SIZE=3

# Commitment level for Solana transactions
COMMITMENT_LEVEL=finalized

# Snipe list settings
USE_SNIPE_LIST=false
SNIPE_LIST_REFRESH_INTERVAL=20000

# Mint renounce checking
CHECK_IF_MINT_IS_RENOUNCED=true

# Auto-sell settings
AUTO_SELL=true
MAX_SELL_RETRIES=5
AUTO_SELL_DELAY=30000  # Delay between selling actions in milliseconds

# Log level
LOG_LEVEL=info

# Profit and loss thresholds
TAKE_PROFIT=200  # Percentage of profit to trigger the sell
STOP_LOSS=50  # Percentage of loss to trigger the stop

# API key for external services like BirdEye
BIRDEYE_API_KEY=4a9711aeXXXXXXXXXXXXXXXXXXXX
